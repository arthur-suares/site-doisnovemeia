generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id        String      @id @default(dbgenerated("gen_random_uuid()"))
  name      String
  email     String   @unique
  password  String 
  createdAt DateTime @default(now())
}

model postagem {
  id        String      @id @default(dbgenerated("gen_random_uuid()"))
  title     String  
}